    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-graphql": "^1.5.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-react": "^7.6.1",
    "nodemon": "^1.14.12",
    "prettier": "^1.10.2",
    "sequelize-cli": "^3.2.0"
  }
