    "compression": "^1.7.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "express-cors": "0.0.3",
    "express-jwt": "^5.3.0",
    "forest-express-sequelize": "^2.4.2",
    "lodash": "^4.17.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "postgraphql": "^3.5.6",
    "sequelize": "^4.33.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
