    "compression": "^1.7.2",
    "connect-redis": "^3.3.3",
    "dataloader": "^1.4.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-cors": "0.0.3",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "forest-express-sequelize": "^2.4.3",
    "graphql": "^0.13.1",
    "graphql-import": "^0.4.4",
    "graphql-tools": "^2.21.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.33.4"
