  "scripts": {
    "start": "node src/index",
    "dev:back": "heroku local",
    "dev:front": "npm run start --prefix client",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "prettier": "prettier --write \"./{src,client/src}/**/*.{js,scss}\"",
    "lint": "eslint --fix \"./{src,client/src}/**/*.js\"",
    "deploy": "git push heroku master",
    "heroku-postbuild": "npm install --dev --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "9.x.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
